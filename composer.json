{
  "name": "kaliop/batch-bundle",
  "type": "symfony-bundle",
  "description": "Features to easily import and export CSV files",
  "keywords": ["kaliop", "command", "console", "import", "export", "batch", "csv"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benoit Wannepain",
      "email": "bwannepain@kaliop.com",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": "^7.1.2",
    "symfony/http-kernel": "^3.3.10",
    "symfony/validator": "^3.3.10",
    "symfony/console": "^3.3.10",
    "symfony/options-resolver": "^3.3.10",
    "symfony/doctrine-bridge": "^3.3.10",
    "doctrine/doctrine-bundle": "^1.7.1",
    "doctrine/orm": "^2.5.11",
    "symfony/filesystem": "^3.3.10",
    "symfony/finder": "^3.3.10",
    "symfony/dependency-injection": "^3.3.10",
    "symfony/config": "^3.3.10",
    "symfony/process": "^3.3.10",
    "monolog/monolog": "1.23.0"
  },
  "suggest": {
    "kaliop/console-bundle": "^1.0"
  },
  "autoload": {
    "psr-4": { "Kaliop\\BatchBundle\\": "src" }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  }
}
